@section scripts {
  <script>
    (function (window) {
      let timePoint = {
        preheatTime: '2018/11/14 13:00',
        firstDay: '2018/11/20 13:00',
        activities: '2018/11/21 13:00',
        endTime: '2018/11/29 13:00'
      }

      function timestampToTime (time) {
        var date = new Date(time);
        let Y = date.getFullYear() + '-';
        let M = (date.getMonth()+1 < 10 ? '0' + (date.getMonth()+1) : date.getMonth()+1) + '-';
        let D = date.getDate() < 10 ? '0' + date.getDate() + ' ' : date.getDate() + ' ';
        let h = date.getHours() < 10 ? '0' + date.getHours() + ':' : date.getHours() + ':';
        let m = date.getMinutes() < 10 ? '0' + date.getMinutes(): date.getMinutes();
        return Y+M+D+h+m;
      }

      let timer = setInterval(() => {
        let curTime = new Date(); 
        let curTimeStamp = curTime.getTime();
        if (curTimeStamp < getTimeStamp(timePoint.firstDay)) {
          preheatTime()
        } else if (curTimeStamp < getTimeStamp(timePoint.activities)) {
          firstDay()
        } else if (curTimeStamp < getTimeStamp(timePoint.endTime)) {
          activities()
        } else {
          let buyBtns = document.querySelectorAll('.buy-btn');
          buyBtns = Array.prototype.slice.call(buyBtns);
          buyBtns.map(btn => {
            btn.setAttribute('href', 'javascript:void(0);');
          })
          console.log('activities end')
          clearTimeout(timer);
        }
      }, 1000);

      function preheatTime() {
        countDown(1);
        extraCheck(1);
        timeTipChange(1);
        changePic(1);
        discount(1);
      }

      function firstDay() {
        countDown(2);
        extraCheck(2);
        timeTipChange(2);
        changePic(1);
        discount(1);
      }

      function activities() {
        countDown(3);
        extraCheck(2);
        timeTipChange(3);
        changePic(2);
        discount(2);
      }

      function countDown(type) {
        let date = new Date();
        let curTimeStamp = date.getTime();
        let endTime;
        switch (type) {
          case 1:
            endTime = getTimeStamp(timePoint.firstDay);
            break;
          case 2:
            endTime = getTimeStamp(timePoint.activities);
            break;
          case 3:
            endTime = getTimeStamp(timePoint.endTime);
            break;
        }
        let diff = endTime - curTimeStamp;
        let timeStr = changeStartTime(diff);
        resetTime(timeStr);
      }
      
      function discount(type) {
        let focusTitle = document.querySelector('.focus-title');
        let url = getStyle('.focus-title', 'background-image');
        let plansTip = document.querySelector('.annual .red-strong');
        switch (type) {
          case 1:
            if (url.indexOf('40') === -1) {
              focusTitle.style.backgroundImage = 'url(/images/black-friday/40.png)';
              plansTip.innerText = '40% off';
            }
            break;
          case 2:
            if (url.indexOf('35') === -1) {
              focusTitle.style.backgroundImage = 'url(/images/black-friday/35.png)';
              plansTip.innerText = '35% off';
            }
            break;
        }
      }

      function getTimeStamp(time) {
        let date = new Date(time);
        return date.getTime()
      }

      function changeStartTime (diff) {
        let timeStr = '';
        let count_down = {};
        if (diff > 0) {
            count_down.day = parseInt(diff / (1000 * 60 * 60 * 24));
            count_down.hour = parseInt((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            count_down.min = parseInt((diff % (1000 * 60 * 60)) / (1000 * 60));

            if (count_down.day>=0) {
                if (count_down.day<10) {
                    count_down.day = "0" + count_down.day;
                }
                timeStr += count_down.day + "天";
            }
            if (count_down.hour<10) {
                count_down.hour = "0" + count_down.hour;
            }
            if (count_down.min<10) {
                count_down.min = "0" + count_down.min;
            }
            return '' + count_down.day + count_down.hour + count_down.min;
        } else {
          return '000000';
        }
      }

      function resetTime(str) {
        let daysTen = document.querySelector('.days-card.ten-digits')
        let daysSingle = document.querySelector('.days-card.single-digits')
        let hoursTen = document.querySelector('.hours-card.ten-digits')
        let hoursSingle = document.querySelector('.hours-card.single-digits')
        let minsTen = document.querySelector('.mins-card.ten-digits')
        let minsSingle = document.querySelector('.mins-card.single-digits')
        daysTen.innerText = str.charAt(0);
        daysSingle.innerText = str.charAt(1);
        hoursTen.innerText = str.charAt(2);
        hoursSingle.innerText = str.charAt(3);
        minsTen.innerText = str.charAt(4);
        minsSingle.innerText = str.charAt(5);
      }

      function extraCheck(type) {
        let extra = document.querySelector('.section-title');
        switch (type) {
          case 2:
            if (extra) {
              extra.parentNode.removeChild(extra);
              let banner = document.querySelector('.banner');
              let height = parseInt(getStyle('.banner', 'height'));
              banner.style.backgroundSize = '100% ' + (height-90) + 'px';
            }
            break;
        }
      }

      function changePic(type) {
        let sectionTitle = document.querySelector('.banner-content .section-title');
        let timeHeader = document.querySelector('.time-header');
        let annualStandard = document.querySelector('.annual-standard-price img');
        let annualProfessional = document.querySelector('.annual-professional-price img');
        let quarterlyStandard = document.querySelector('.quarterly-standard-price img');
        let quarterlyProfessional = document.querySelector('.quarterly-professional-price img');
        let monthlyStandard = document.querySelector('.monthly-standard-price img');
        let monthlyProfessional = document.querySelector('.monthly-professional-price img');

        switch (type) {
          case 1:
            if (annualStandard.getAttribute('src') !== '/images/black-friday/$629.png') {
              annualStandard.setAttribute('src', '/images/black-friday/$629.png');
            }
            if (annualProfessional.getAttribute('src') !== '/images/black-friday/$1529.png') {
              annualProfessional.setAttribute('src', '/images/black-friday/$1529.png');
            }
            if (quarterlyStandard.getAttribute('src') !== '/images/black-friday/$170.png') {
              quarterlyStandard.setAttribute('src', '/images/black-friday/$170.png');
            }
            if (quarterlyProfessional.getAttribute('src') !== '/images/black-friday/$467.png') {
              quarterlyProfessional.setAttribute('src', '/images/black-friday/$467.png');
            }
            if (monthlyStandard.getAttribute('src') !== '/images/black-friday/$80.png') {
              monthlyStandard.setAttribute('src', '/images/black-friday/$80.png');
            }
            if (monthlyProfessional.getAttribute('src') !== '/images/black-friday/$179.png') {
              monthlyProfessional.setAttribute('src', '/images/black-friday/$179.png');
            }
            break;

          case 2:
            if (annualStandard.getAttribute('src') !== '/images/black-friday/$699.png') {
