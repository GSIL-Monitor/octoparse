              timeTip.innerText = 'Sale ending in';
            }
            break;
        }
      }

      function getStyle(selector, style) {
        let dom = document.querySelector(selector);
        return dom.style[style] ? dom.style[style] : dom.currentStyle ? dom.currentStyle[style]: getComputedStyle(dom , null)[style];
      }
    })(window);

    (function(window) {
      let formDOM = document.getElementById('contact-form')
      let btnSubmit = document.getElementById('btn-submit')

      btnSubmit.addEventListener('click', e => {
        let i = formDOM.childNodes.length
        let hasInvaildValue = false

        while(i-- > 0) {
          let dom = formDOM.childNodes[i]
          if ('value' in dom) {
            if (dom.value.trim() === '') {
              hasInvaildValue = true
              break
            }
          }
        }

        if (hasInvaildValue) {
          return alert('all info can not empty!')
        }

        let formData = new FormData(formDOM);
        let xhr = new XMLHttpRequest();

        xhr.open('post', '/Octopus/ContactSale/support');

        xhr.onreadystatechange = () => {
          if (xhr.readyState === 4) {
            if (xhr.status === 200 && JSON.parse(xhr.responseText).success===true) {
              window.location.href = "/committed-successful";
              return;
            } else {
              alert('Unable to submit your request. Please try again.');
              btnSubmit.removeAttribute('disabled');
            }
          }
        }

        btnSubmit.setAttribute('disabled', 'disable');
        xhr.send(formData);
      })
    })(window)
  </script>
