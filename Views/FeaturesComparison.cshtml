		.table-top3-p {height:100%;}
		.table-top3-p p {padding-bottom:16px;font-size:13px;}
		.first-color {color:#999;}
		.last {border-right:none;}
		.border-none {border-bottom:none;}
		.padding-none {padding:0;}
		.p-bold {font-weight:bold;}
		.color-green {color:#2cb159}
		.color-red {color:red;}
	</style>
}

